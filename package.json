    "cross-env": "^7.0.3",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "electron-builder build --mac --win --publish never",
    "deploy": "electron-builder build --mac --win --publish always",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm run start\" \"npm run electron:start\"",
    "build-prod": "react-scripts build && electron-builder --win",
    "build:publish": "react-scripts build && electron-builder --win --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "author": "<FrameInCam>",
  "license": "ISC",
  "build": {
    "appId": "imageproof.ai",
    "productName": "ImageProof",
    "copyright": "Copyright Â© 2020 your company",
    "directories": {
      "output": "electron-build",
      "buildResources": "resources"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENCE.rtf"
    },
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis"
        }
      ],
      "icon": "public/icon.ico"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
